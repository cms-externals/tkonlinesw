 </xs:restriction>
</xs:simpleType>

<xs:simpleType name="halBusAdaptorType">
 <xs:restriction base="xs:string">
  <xs:enumeration value="SBS"/>
  <xs:enumeration value="CAEN_USB"/>
  <xs:enumeration value="CAEN_PCI"/>
  <xs:enumeration value="VXI"/>
